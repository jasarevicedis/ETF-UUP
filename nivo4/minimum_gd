#include <stdio.h>


int main() {
    int n=10;
    int i,j;
    int matrica[10][10];
    int min = 100; //neki veliki broj ,koji ce se smanjivati, mnogo je lakse ovako
	for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            scanf(" %d", &matrica[i][j]);
            
            
            scanf("\n");
        }
        
    }

    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            if(i==j && matrica[i][j]<=min){
                min = matrica[i][j];
            }
            
        }
    }
    printf("%d", min);
	return 0;
}
