#include <stdio.h>

int main() {
    int n, i, j;
	printf("Unesite n: \n");
    scanf("%d", &n);
    if(n<=2 || n> 10){
        printf("Uneseni broj nije u intervalu!");
        return 0;
    }
    else{
        for(i=1; i<=n; i++){
            for(j=1; j<=n; j++){
                
                if(i*j <10){
                    printf("%d   ", i*j);
                }
                else if(i*j >=10 && i*j<100){
                    printf("%d  ", i*j);
                }
                else {
                    printf("%d ", i*j);
                }
                
                
            }
            printf("\n");

            
        }
    }
	return 0;
}
