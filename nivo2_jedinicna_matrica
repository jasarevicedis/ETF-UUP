#include <stdio.h>

int main() {
    int n, r, i, j, k;
	printf("Unesite n: \n");
    scanf("%d", &n);
    printf("Unesite sirinu ispisa: ");
    scanf("%d", &r);
    for(i=1;i<=n;i++){
        for(j=1; j<=n; j++){
            for(k=1; k<=r;k++){
                printf(" ");
            }
            if(i==j){
                printf("1");
            }
            else {
                printf("0");
            }
        }
        printf("\n");
    }
	return 0;
}
